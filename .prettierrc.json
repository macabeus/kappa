{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": [
    "^((@configurations)|(@db)|(@decomp-permuter)|(@decompme)|(@language-model-tools)|(@m2c)|(@objdiff)|(@prompt-builder)|(@providers)|(@test)|(@utils)|(@webview)|~)\/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderSeparation": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
